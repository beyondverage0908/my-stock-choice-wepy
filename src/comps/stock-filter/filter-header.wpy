<style lang="less">
@font-size24: 24rpx;
@font-size26: 26rpx;
@font-size28: 28rpx;
@font-size30: 30rpx;
@main-blue: #5291f5;
.filter-header-container {
  .header-bg {
    height: 160rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
    .search {
      display: flex;
      align-items: center;
      height: 100%;
      .avatar-warp {
        display: flex;
        justify-content: center;
        flex: 1;
        .h-wrap {
          overflow: hidden;
          width: 80rpx;
          height: 80rpx;
          border-radius: 40rpx;
        }
      }
      .input-wrap {
        flex: 4;
        box-sizing: border-box;
        height: 80rpx;
        padding: 0 0 0 20rpx;
        margin-right: 15rpx;
        margin-left: 0rpx;
        background-color: #fff;
        border-radius: 40rpx;
        .input {
          box-sizing: border-box;
          height: 100%;
          padding: 0 0 0 5rpx;
          font-size: @font-size30;
          background-color: #fff;
          border-radius: 40rpx;
        }
      }
      .filter-warp {
        display: flex;
        justify-content: center;
        align-items: center;
        flex: 1;
        height: 80rpx;
        margin-right: 15rpx;
        color: #fff;
        font-size: @font-size26;
        background-color: @main-blue;
        border-radius: 30rpx;
        .icon {
          width: 30rpx;
          margin-top: 7rpx;
        }
      }
    }
    // 搜索
    .search-board {
      position: absolute;
      top: 150rpx;
      z-index: 10;
      width: 100%;
      min-height: 90vh;
      background: #fff;
      .search-item {
        padding: 18rpx 0;
        border-bottom: 1rpx solid #f6f6f6;
        .info {
          margin-left: 25rpx;
          font-size: 25rpx;
        }
      }
    }
  }
}
</style>
<template>
    <div class="filter-header-container">
        <div class="header-bg">
            <div class="search">
                <div class="avatar-warp" @click="handleAvatarClick">
                    <div class="h-wrap">
                        <open-data type="userAvatarUrl"></open-data> 
                    </div>
                </div>
                <div class="input-wrap">
                    <input 
                        class="input" 
                        type="text" 
                        v-model="inputText" 
                        bindinput="handleChangeInput" 
                        placeholder="请输入公司名称/代码" />
                </div>
                <div class="filter-warp" @click="handlePickFilter">
                    <img class="icon" src="../../assets/filter.svg"/>
                    <span>选股器</span>
                </div>
            </div>
            <div class="search-board" v-if="stockList.length && !startSearch">
                <div class="search-item" v-for="item in stockList" @tap="handleGetStockSearch(item)" :key="item.zqjc">
                    <span class="info">{{item.zqjc}}({{item.zqdm}})</span>
                </div>
            </div>
        </div>
    </div>
</template>
<script>
import wepy from '@wepy/core';
import { 
    getSearchStockInfo,
    getStockDefault, 
    getStockInfo
} from '@/api/home'
wepy.component({
    props: {},
    data: {
        inputText: '',
        stockList: [],
        startSearch: false
    },
    methods: {
        handleAvatarClick() {
            wx.navigateTo({ url: '/pages/stock-list' })
        },
        handlePickFilter() {
            const pages = getCurrentPages()
            console.log(pages)
            wx.navigateBack({ url: '/pages/stock-filter' })
        },
        handleChangeInput() {
            this.startSearch = false;
			if (this.inputText) {
                try {
                    this.getSearchStockInfo(this.inputText).then(res => {
                        this.stockList = res
                    })
                } catch (error) {
                    console.log(error)
                    this.stockList = []
                }
			} else {
				this.stockList = [];
			}
        },
        getSearchStockInfo(searchText) {
			return new Promise((resolve, reject) => {
				getSearchStockInfo(searchText).then(res => {
					if (res.data.success) {
						resolve(res.data.data);
					} else {
						reject()
					}
				})
			});
        },
        handleGetStockSearch(stock) {
            this.getStockDetailInfo(stock.zqdm)
        },
        async getStockDetailInfo(stockCode) {
            try {
                const { data } = await getStockInfo(stockCode)
                this.startSearch = true
                this.inputText = ''
                if (data.success) {
                    this.$emit('on-picked-stock', data.data)
                } else {
                    wx.showToast({
                        title: res.data.message,
                        icon: 'none',
                        duration: 2000
                    })
                }
            } catch(e) {
                this.startSearch = true
                wx.showToast({ title: '网络异常', icon: 'none', duration: 2000 })
            }
        }
    }
})
</script>
